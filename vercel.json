{
  "version": 2,
  "builds": [
    {
      "src": "desktop-app/dist/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "desktop-app/api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/desktop-app/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/desktop-app/dist/$1"
    }
  ]
}