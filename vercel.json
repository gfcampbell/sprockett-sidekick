{
  "version": 2,
  "functions": {
    "desktop-app/api/**/*.js": {
      "runtime": "@vercel/node@3.0.0"
    }
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/desktop-app/api/:path*"
    },
    {
      "source": "/:path*",
      "destination": "/desktop-app/dist/:path*"
    }
  ]
}